{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "recurseDepth": 10,
    "source": {
        "include": "src/",
        "exclude": "",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|test.js$|src/index.js$|_lib/)"
    },
    "sourceType": "module",
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "template": "node_modules/docdash",
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false,
            "includeDate": false,
            "useLongnameInNav": true
        }
    },
    "docdash": {
        "static": true,
        "sort": true,
        "disqus": "",
        "search": true,
        "collapse": false,
        "typedefs": true,
        "removeQuotes": "none"
    }
}
